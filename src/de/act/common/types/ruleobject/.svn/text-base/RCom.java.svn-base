package de.act.common.types.ruleobject;

/**
 * @author Testong
 */

public class RCom implements java.io.Serializable{

	private static final long serialVersionUID = -7853184717648797595L;
	private long RId;
	private RBase RBase;
	private Integer comBool;
	private long comId;
	private long annId;
	
	public RCom(){
		
	}
	
	public RCom(RBase RBase) {
		this.RBase = RBase;
	}

	public RCom(RBase RBase, Integer comBool, long comId, long annId) {
		this.RBase = RBase;
		this.comBool = comBool;
		this.comId = comId;
		this.annId = annId;
	}

	public long getRId() {
		return RId;
	}

	public void setRId(long id) {
		RId = id;
	}

	public RBase getRBase() {
		return RBase;
	}

	public void setRBase(RBase base) {
		RBase = base;
	}

	public Integer getComBool() {
		return comBool;
	}

	public void setComBool(Integer comBool) {
		this.comBool = comBool;
	}

	public long getComId() {
		return comId;
	}

	public void setComId(long comId) {
		this.comId = comId;
	}

	public long getAnnId() {
		return annId;
	}

	public void setAnnId(long annId) {
		this.annId = annId;
	}
	
}
