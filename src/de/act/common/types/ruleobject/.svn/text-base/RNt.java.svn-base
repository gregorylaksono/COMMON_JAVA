package de.act.common.types.ruleobject;

/**
 * @author Henry
 */
public class RNt implements java.io.Serializable {

	private static final long serialVersionUID = 7696531559863371879L;
	private long RId;
	private RBase RBase;
	private String ntCa;
	private String ntId;

	public RNt() {
		
	}

	public RNt(RBase RBase) {
		this.RBase = RBase;
	}

	public RNt(RBase RBase, String ntCa, String ntId) {
		this.RBase = RBase;
		this.ntCa = ntCa;
		this.ntId = ntId;
	}

	public long getRId() {
		return this.RId;
	}

	public void setRId(long RId) {
		this.RId = RId;
	}

	public RBase getRBase() {
		return this.RBase;
	}

	public void setRBase(RBase RBase) {
		this.RBase = RBase;
	}

	public String getNtCa() {
		return this.ntCa;
	}

	public void setNtCa(String ntCa) {
		this.ntCa = ntCa;
	}

	public String getNtId() {
		return this.ntId;
	}

	public void setNtId(String ntId) {
		this.ntId = ntId;
	}

}
