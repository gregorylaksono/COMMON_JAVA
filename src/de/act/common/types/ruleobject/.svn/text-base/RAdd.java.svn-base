package de.act.common.types.ruleobject;

/**
 * @author Henry
 */
public class RAdd implements java.io.Serializable {

	private static final long serialVersionUID = -8752514327402636529L;
	private long RId;
	private RBase RBase;
	private Integer addBool;
	private Integer addId;
	private String addType;
	private String addSpecType;

	public RAdd() {
		
	}

	public RAdd(RBase RBase) {
		this.RBase = RBase;
	}

	public RAdd(RBase RBase, Integer addBool, Integer addId, String addType, String addSpecType) {
		this.RBase = RBase;
		this.addBool = addBool;
		this.addId = addId;
		this.addType = addType;
		this.addSpecType = addSpecType;
	}

	public long getRId() {
		return RId;
	}

	public void setRId(long id) {
		RId = id;
	}

	public RBase getRBase() {
		return RBase;
	}

	public void setRBase(RBase base) {
		RBase = base;
	}

	public Integer getAddBool() {
		return addBool;
	}

	public void setAddBool(Integer addBool) {
		this.addBool = addBool;
	}

	public Integer getAddId() {
		return addId;
	}

	public void setAddId(Integer addId) {
		this.addId = addId;
	}

	public String getAddType() {
		return addType;
	}

	public void setAddType(String addType) {
		this.addType = addType;
	}

	public String getAddSpecType() {
		return addSpecType;
	}

	public void setAddSpecType(String addSpecType) {
		this.addSpecType = addSpecType;
	}
}