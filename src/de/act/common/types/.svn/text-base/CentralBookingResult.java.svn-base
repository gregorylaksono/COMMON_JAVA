package de.act.common.types;

import java.util.Date;

public class CentralBookingResult {
	private final String bkgId;
	private final Long sFltId;
	private final Date   date;
	private final String status;

	public CentralBookingResult(String bkgId, Long sFltId, Date date, String status) {
		this.bkgId  = bkgId;
		this.sFltId = sFltId;
		this.date   = date;
		this.status = status;
	}

	public String getBkgId() {
		return this.bkgId;
	}
	
	public Date getDate() {
		return this.date;
	}
	
	public Long getSFltId() {
		return this.sFltId;
	}
	
	public String getStatus() {
		return this.status;
	}
} // class CentralBookingResult
