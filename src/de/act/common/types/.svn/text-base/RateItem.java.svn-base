package de.act.common.types;

import java.io.Serializable;
import java.util.Date;

@org.jboss.cache.aop.annotation.PojoCacheable
public final class RateItem implements Serializable {

	private static final long serialVersionUID = 4143577310110545308L;
	private int    index   = -1;
	private String comId   = null; //HENRY it should be Long
	private long   annId   = -1L;
	private String itemNo  = null;
	private long   ruleId  = -1;
	private Date   vadFrom = null;
	private Date   vadTo   = null;

	public String getItemNo() {
		return this.itemNo;
	}

	public void setItemNo(final String itemNo) {
		this.itemNo = itemNo;
	}

	public long getAnnId() {
		return this.annId;
	}

	public void setAnnId(final long annId) {
		this.annId = annId;
	}

	public String getComId() {
		return this.comId;
	}

	public void setComId(final String comId) {
		this.comId = comId;
	}

	public int getIndex() {
		return this.index;
	}

	public void setIndex(final int index) {
		this.index = index;
	}

	public long getRuleId() {
		return this.ruleId;
	}

	public void setRuleId(final long ruleId) {
		this.ruleId = ruleId;
	}

	public Date getVadFrom() {
		return this.vadFrom;
	}

	public void setVadFrom(final Date vadFrom) {
		this.vadFrom = vadFrom;
	}

	public Date getVadTo() {
		return this.vadTo;
	}

	public void setVadTo(final Date vadTo) {
		this.vadTo = vadTo;
	}
}
