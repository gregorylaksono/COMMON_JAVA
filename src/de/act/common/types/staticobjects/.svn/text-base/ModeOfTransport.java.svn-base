package de.act.common.types.staticobjects;

import java.io.Serializable;

public class ModeOfTransport implements Serializable
{
	private int motId;
	private String motName;
	private String motDesc;

	public int getMotId()
	{
		return motId;
	}

	public void setMotId(int motId)
	{
		this.motId = motId;
	}

	public String getMotName()
	{
		return motName;
	}

	public void setMotName(String motName)
	{
		this.motName = motName;
	}

	public String getMotDesc()
	{
		return motDesc;
	}

	public void setMotDesc(String motDesc)
	{
		this.motDesc = motDesc;
	}

}
